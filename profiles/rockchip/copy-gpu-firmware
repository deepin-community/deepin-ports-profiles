i#!/bin/sh
set -e
PREREQ=""
prereqs()
{
        echo "${PREREQ}"
}

case "${1}" in
        prereqs)
                prereqs
                exit 0
                ;;
esac

. /usr/share/initramfs-tools/hook-functions

mkdir -p ${DESTDIR}/lib/firmware/arm/mali/arch10.8/

cp -a /lib/firmware/arm/mali/arch10.8/* ${DESTDIR}/lib/firmware/arm/mali/arch10.8/
